import spacy

nlp = spacy.load("en_core_web_sm")
text = "The quick brown fox jumps over the lazy dog"
doc = nlp(text)

for np in doc.noun_chunks:
    print("Noun Phrase:", np.text)
    for token in np:
        if token.pos_ == "NOUN":
            print("Meaning:", token.text, "-", token.pos_)
    print()
